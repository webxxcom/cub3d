{
  "scopeName": "source.cub",
  "patterns": [
    { "match": "#.*$", "name": "comment.line.cub" },
    { "match": "\\b(NO|SO|WE|EA)\\b", "name": "keyword.control.cub" },
    { "match": "\\b(F|C)\\b", "name": "variable.language.cub" },
    { "match": "\\b(WALL|DOOR|LIGHT|SPRITE)\\b", "name": "storage.type.cub" },
    { "match": "\\b(NORTH|SOUTH|EAST|WEST)\\b", "name": "entity.name.type.cub" },
    { "match": "\\b[0-9|N|W|S|E]+\\b", "name": "constant.numeric.cub" },
    { "match": "\\btextures/[A-Za-z0-9_./-]+\\.xpm\\b", "name": "string.quoted.cub" },
    { "match": "\\[[A-Za-z0-9_]+\\]", "name": "entity.name.section.cub" }
  ]
}

