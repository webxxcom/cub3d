{
  "name": "cub-syntax",
  "displayName": "CUB Syntax Highlighting",
  "description": "Syntax highlighting for .cub map files",
  "version": "0.0.1",
  "engines": { "vscode": "^1.50.0" },
  "contributes": {
    "languages": [
      {
        "id": "cub",
        "aliases": ["CUB", "cub"],
        "extensions": [".cub"]
      }
    ],
    "grammars": [
      {
        "language": "cub",
        "scopeName": "source.cub",
        "path": "./syntaxes/cub.tmLanguage.json"
      }
    ]
  }
}

